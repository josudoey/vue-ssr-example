{
  "name": "vue-ssr-example",
  "version": "1.0.0",
  "description": "example project",
  "scripts": {
    "ts-check": "tsc --noEmit",
    "ncu": "npx npm-check-updates",
    "build:example-vue2": "NODE_ENV=production node ./scripts/build-example-vue2.mjs",
    "build:example-vue3": "NODE_ENV=production node ./scripts/build-example-vue3.mjs",
    "build": "npm run build:example-vue2&&npm run build:example-vue3",
    "start": "NODE_ENV=production node ./main.mjs",
    "build:dev:example-vue2": "node ./scripts/build-example-vue2.mjs",
    "build:dev:example-vue3": "node ./scripts/build-example-vue3.mjs",
    "build:dev": "npm run build:dev:example-vue2&&npm run build:dev:example-vue3",
    "dev": "DEBUG=app:* nodemon --trace-warnings -e js,cjs,mjs,css,html,pug --ignore ./dist.dev/** --exec 'npm run build:dev&&node ./main.mjs'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josudoey/vue-ssr-example.git"
  },
  "author": "Joey Fang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/josudoey/vue-ssr-example/issues"
  },
  "packageManager": "pnpm@6.29.1",
  "homepage": "https://github.com/josudoey/vue-ssr-example#readme",
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/eslint-parser": "^7.17.0",
    "@tsconfig/node16": "^1.0.3",
    "@types/node": "^18.7.21",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.24.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.0.1",
    "nodemon": "^2.0.15",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "debug": "^4.3.3",
    "~example-server": "workspace:*",
    "~example-vue2": "workspace:*",
    "~example-vue3": "workspace:*"
  }
}
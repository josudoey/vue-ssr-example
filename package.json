{
  "name": "vue-ssr-example",
  "version": "1.0.0",
  "description": "example project",
  "scripts": {
    "build:ssr": "NODE_ENV=production webpack --config ./vue/webpack.config.ssr.js",
    "build:asset": "NODE_ENV=production webpack --config ./vue/webpack.config.asset.js",
    "build": "npm run build:asset;npm run build:ssr",
    "start": "NODE_ENV=production node ./main.mjs",
    "dev": "DEBUG=app:* nodemon ./dev.mjs --trace-warnings -e js,mjs,css,html,pug --ignore ./vue/dist/* --ignore ./vue/dist.dev/*"
  },
